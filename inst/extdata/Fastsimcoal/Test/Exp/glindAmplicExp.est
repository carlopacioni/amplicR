[PARAMETERS]
//#isInt? #name   #dist.#min  #max
//all Ns are in number of haploid individuals
1  NCUR  unif 10  100000 output
1  NEUR  unif 10  100000 output
1 NANC   unif 10  100000 output
1 TPOPCONST unif 41 1000 output

[COMPLEX PARAMETERS]
0 RATIOEUR     = NEUR/NCUR output
0 RESHIST  = NANC/NEUR hide
0 LOGRATIOEC = log(RATIOEUR) output
0 GEUR = LOGRATIOEC/40 output
0 RANCE = NANC/NEUR output
0 LNRAE = log(RANCE) output
0 GHIST = LNRAE/TPOPCONST output